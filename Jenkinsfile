pipeline {
    agent any 
    
    stages {
        stage('build') {
            steps {
                echo 'my pipeline execution: building the application...'
            }
        }
        
        stage('test') {
            steps {
                echo 'my pipeline execution: testing the application...'
            }
        }
        
        stage('deploy') {
            steps {
                echo 'my pipeline execution: deploying the application...'
            }
        }
       
    }
}
